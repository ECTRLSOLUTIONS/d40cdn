{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prefer-const": 0,
        "no-unused-vars": "warn"
    },
    "globals": {
        "_": "readonly",
        "Vue": "readonly",
        "VueRouter": "readonly",
        "Liferay": "readonly",
        "SuggestoMap": "readonly",
        "axios": "readonly",
        "filterConfig": "readonly"
    }
}
